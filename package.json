{
    "name": "g-vote",
    "version": "1.0.0",
    "scripts": {
        "build": "cd client && npm install && npm run build && cd ../server && npm install",
        "start": "node server/index.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && node index.js",
        "client": "cd client && npm run dev"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}